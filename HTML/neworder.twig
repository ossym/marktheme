<script src="https://cdn-script.com/ajax/libs/jquery/3.7.1/jquery.js" type="text/javascript"></script>

<script>
    const categories = {{  extended_categories | json_encode() }};
	const defaultIcon = '<div class="icon-box"><i class="far fa-star"></i></div>';
	$(document).ready(function() {
		$.ajax({
			url: "/get-services",
			async: !0,
			method: "GET",
			success: function success(e) {
				if(window.modules.siteOrder !=null ){
				window.modules.siteOrder.services = e.data || {};
				}
				
			}
		});
		var services = window.modules.siteOrder.services;
        
		initServices();
		updateSelectedDescription();
		$("#orderform-service").change(function() {
			initServices();
			updateSelectedDescription();
		});

		$("#orderform-country").change(function() {
			if ($('.tab-btn.active-tab').data('term') === 'country') {
				const country = $(this).val();
				const options = filterCategories('new', country);
				$('#orderform-category').html(options);
				const firstCat = $('#orderform-category option:first').val();
				$('#orderform-category').val(firstCat).trigger('change');
			}
			initServices();
			updateSelectedDescription();
		});
        $('.nwo-cat-btn').on('click', function() {
            const activeTab = $('.tab-btn.active-tab').data('term');
            if (activeTab === 'country')
                return;
            $('.nwo-cat-btn').removeClass('active is-active').attr('aria-pressed', 'false');
            $(this).addClass('active is-active').attr('aria-pressed', 'true');
			const term = $(this).data('change-cat');
			const options = term ? filterCategories(term) : filterCategories('new');
			$('#orderform-category').html(options);
			initServices();
			$('#orderform-category').trigger('change');
		});
		$('#services-dropdown-btn').on('click', function(e) {
			e.stopPropagation();
			const isExpanded = $(this).attr('aria-expanded') === 'true';
			$(this).attr('aria-expanded', !isExpanded);
			$('#services-dropdown-list').toggleClass('hidden');
		});
		// Initialize active tab styling on page load
		$('.tab-btn.active-tab').each(function() {
			const $activeTab = $(this);
			// Apply inline styles to active tab on load
			$activeTab.css({
				'border-color': '#10c3f4',
				'background': 'linear-gradient(135deg, #10c3f4 0%, #00acc1 100%)',
				'color': 'white'
			});
			// Update text color to white
			$activeTab.find('span').css('color', 'white');
			// Update SVG icon
			$(this).find('svg path').attr('fill', 'white');
		});
		$('.tab-btn:not(.active-tab)').each(function() {
			$(this).find('svg path').attr('fill', 'currentColor');
		});

		// Handle service button clicks and prevent dropdown from closing
		$(document).on('click', '#services-dropdown-list', function(e) {
			// Find the closest button with data-service-id (works even if clicking on child elements)
			let $button = $(e.target).closest('button[data-service-id]');
			
			// If closest didn't work, try parents
			if ($button.length === 0) {
				$button = $(e.target).parents('button[data-service-id]').first();
			}
			
			// If still not found, check if the target itself is a button
			if ($button.length === 0 && $(e.target).is('button[data-service-id]')) {
				$button = $(e.target);
			}
			
			if ($button.length > 0) {
				// Handle button click
				e.stopPropagation();
				e.preventDefault();
				const serviceId = $button.data('service-id');
				if (serviceId) {
					selectOrder(serviceId);
				}
				return false;
			} else {
				// Prevent dropdown from closing when clicking inside (but not on a button)
				e.stopPropagation();
			}
		});

		// Close dropdown when clicking outside
		$(document).on('click', function(e) {
			if (!$(e.target).closest('#services-dropdown-btn, #services-dropdown-list').length) {
				$('#services-dropdown-list').addClass('hidden');
				$('#services-dropdown-btn').attr('aria-expanded', 'false');
			}
		});
		$('.tab-btn').on('click', function() {
			const $tab = $(this);
			// Remove active state from all tabs
			$('.tab-btn').each(function() {
				const $thisTab = $(this);
				$thisTab.removeClass('active-tab border-primary-500 bg-primary-500 text-white').addClass(
					'border-gray-300 bg-white text-gray-600');
				// Remove inline styles
				$thisTab.css({
					'border-color': '',
					'background': '',
					'background-color': '',
					'color': ''
				});
				// Update text color for inactive tabs
				$thisTab.find('span').css('color', '');
				// Update SVG icons for inactive tabs
				$thisTab.find('svg path').attr('fill', 'currentColor');
			});
			// Add active state to clicked tab
			$tab.removeClass('border-gray-300 bg-white text-gray-600').addClass('active-tab border-primary-500 bg-primary-500 text-white');
			// Add inline styles for active tab
			$tab.css({
				'border-color': '#10c3f4',
				'background': 'linear-gradient(135deg, #10c3f4 0%, #00acc1 100%)',
				'color': 'white'
			});
			// Update text color to white for active tab
			$tab.find('span').css('color', 'white');
			// Update SVG icon for active tab
			$tab.find('svg path').attr('fill', 'white');
			const term = $tab.data('term');
			if (term === 'country') {
				$('.nCountry').removeClass('hidden');
				$('#new-order-cats').hide();
				const options = getCountryOptions();
				$('#orderform-country').html(options);
				$('#orderform-country').trigger('change');
			} else {
				$('.nCountry').addClass('hidden');
				$('#new-order-cats').show();
				const options = filterCategories(term);
				$('#orderform-category').html(options).trigger('change');
			}
		});
	});


    function updateSelectedDescription() {
        const serviceId = $('#orderform-service').val();
        const service = window.modules.siteOrder.services[serviceId];
        const description = service && service.description ? service.description : '<em>No description available</em>';
        $('#description_container').html(description);
	}

	function initServices() {
		const $servicesSelect = $('#orderform-service');
		const $dropdownMenu = $('#services-dropdown-list');
		$dropdownMenu.empty();
		let added = false,
			firstMatchId = null,
			firstMatchText = null,
			foundSelected = false,
			currentSelected = $servicesSelect.val();
		// Get active tab and selected category
		const activeTab = $('.tab-btn.active-tab').data('term') || 'new';
		const selectedCategoryId = $('#orderform-category').val();
		const selectedCountry = $('#orderform-country').val();
		const lowerCountry = selectedCountry ? selectedCountry.toLowerCase() : '';
		// Iterate only on <select> options
		$servicesSelect.find('option').each(function() {
			const serviceId = $(this).val();
			const serviceName = $(this).text();
			// Get service info from global object to apply filter
			const service = window.modules.siteOrder.services[serviceId];
			if (!service)
				return;
			const categoryName = (categories.find(cat => cat.id == service.cid) || {}).name || '';
			const matchCategory = service.cid == selectedCategoryId;
			const matchCountry = activeTab === 'country' && selectedCountry ? (serviceName.toLowerCase().includes(
				lowerCountry) || categoryName.toLowerCase().includes(lowerCountry)) : true;
			const matchType = activeTab === 'auto' ? parseInt(service.type) === 100 : true;
			const matchFav = activeTab === 'fav' ? Boolean(service.favorite) : true;
			if (!(matchCategory && matchCountry && matchType && matchFav))
				return;
			if (!firstMatchId) {
				firstMatchId = serviceId;
				firstMatchText = serviceName;
			}
			if (serviceId === currentSelected) {
				foundSelected = true;
			}
			const ico = getIcon(serviceName);
			const displayName = serviceName.replace(new RegExp(`^${serviceId}\\s*[-:|]*\\s*`), '').trim();
			const isSelected = serviceId === currentSelected;
			const selectedClass = isSelected ? 'selected' : '';
			
			// Build service item HTML
			let serviceItem = `<button type="button"
                class="service-item ${selectedClass} w-full text-left"
                data-service-id="${serviceId}"
                role="option"
                aria-selected="${isSelected}"
            >`;
			serviceItem += `<div class="flex items-center gap-2">`;
			serviceItem += `<span class="service-item-icon">${ico}</span>`;
			serviceItem += `<span class="service-item-badge">${serviceId}</span>`;
			serviceItem += `<div class="service-item-content">`;
			serviceItem += `<span class="service-item-name">${displayName}</span>`;
			serviceItem += `<div class="service-item-badges">`;
			
			serviceItem += `</div>`;
			serviceItem += `</div>`;
			
			if (service.time || service.charge) {
				serviceItem += `<div class="service-item-price">`;
				if (service.time) {
					serviceItem += `<span>{${service.time}}</span>`;
				}
				if (service.charge) {
					serviceItem += `<span> - $${parseFloat(service.charge).toFixed(2)} per 1000</span>`;
				}
				serviceItem += `</div>`;
			}
			
			serviceItem += `</div>`;
			serviceItem += `</button>`;
			
			$dropdownMenu.append(serviceItem);
			added = true;
		});
		if (!added) {
			$dropdownMenu.append('<div class="empty-message px-4 py-3 text-sm text-gray-500">No services available</div>');
			$('#order-services').text('Services List');
			$servicesSelect.prop('selectedIndex', -1);
		} else {
			if (!foundSelected && firstMatchId) {
				$servicesSelect.val(firstMatchId);
				setDropDownButton(firstMatchId, firstMatchText);
			} else {
				const selectedOption = $servicesSelect.find('option:selected');
				setDropDownButton(selectedOption.val(), selectedOption.text());
			}
		}
		customTooltip();
	}

	function customTooltip() {
		var new_posts =
			`New posts <i class="fas fa-question-circle" data-bs-toggle="tooltip" data-bs-placement="top" title="Set the number of future posts to include. Leave at 0 if not needed."></i>`;
		var old_posts =
			`Old posts <i class="fas fa-question-circle" data-bs-toggle="tooltip" data-bs-placement="top" title="Set the number of past posts to include. Leave at 0 if not needed."></i>`;
		var quantity =
			`Quantity <i class="fas fa-question-circle" data-bs-toggle="tooltip" data-bs-placement="top" title="Choose the minimum and maximum amount. You'll get a random number between these values."></i>`;
		var delay =
			`Delay <i class="fas fa-question-circle" data-bs-toggle="tooltip" data-bs-placement="top" title="Set a delay to receive the ordered amount to your post after they are published."></i>`;
		var expiry =
			`Expiry <i class="fas fa-question-circle" data-bs-toggle="tooltip" data-bs-placement="top" title="Set when you want this subscriptiontoexpire."></i>`;
		var n_posts_label = document.querySelector('label[for="field-orderform-fields-posts"]');
		var o_posts_label = document.querySelector('label[for="field-orderform-fields-old_posts"]');
		var quantity_label = document.querySelector('label[for="order_count"]');
		var delay_label = document.querySelector('label[for="field-orderform-fields-delay"]');
		var expiry_label = document.querySelector('label[for="field-orderform-fields-expiry"]');
		if (n_posts_label || o_posts_label || quantity_label || delay_label || expiry_label) {
			n_posts_label.innerHTML = new_posts;
			o_posts_label.innerHTML = old_posts;
			quantity_label.innerHTML = quantity;
			delay_label.innerHTML = delay;
			expiry_label.innerHTML = expiry;
		}
	}

	function selectOrder(val) {
		$('#orderform-service').val(val);
		$("#orderform-service").trigger("change");
		var selectedOption = $("#orderform-service option[value='" + val + "']");
		if (selectedOption.length > 0) {
			setDropDownButton(selectedOption.val(), selectedOption.text());
			// Close dropdown after selection
			$('#services-dropdown-list').addClass('hidden');
			$('#services-dropdown-btn').attr('aria-expanded', 'false');
			// Update selected state in dropdown
			$('#services-dropdown-list .service-item').removeClass('selected');
			$('#services-dropdown-list .service-item[data-service-id="' + val + '"]').addClass('selected');
		} else {
			console.error('Selected option not found.');
		}
	}

	function setDropDownButton(service_id, service_name) {
		const service = window.modules.siteOrder.services[service_id];
		let icon = getIcon(service_name);
		const displayName = service_name.replace(new RegExp(`^${service_id}\\s*[-:|]*\\s*`), '').trim();
		
		// Build service info HTML
		let serviceInfo = `<div class=" items-center gap-2 flex-wrap">`;
		serviceInfo += `<span class="flex-shrink-0">${icon}</span>`;
		serviceInfo += `<span class="bg-orange-500 text-white rounded px-2 py-0.5 text-xs font-bold flex-shrink-0">${service_id}</span>`;
		serviceInfo += `<span class="font-medium">${displayName}</span>`;
		
		if (service) {
			if (service.time) {
				serviceInfo += `<span class="flex items-center gap-1 text-gray-600"><svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd"/></svg> {${service.time}}</span>`;
			}
			if (service.charge) {
				serviceInfo += `<span class="text-primary-500 font-semibold">- $${parseFloat(service.charge).toFixed(2)} per 1000</span>`;
			}
		}
		
		serviceInfo += `</div>`;
		$("#order-services").html(serviceInfo);
	}

	function filterCategories(term, country) {
		const services = Object.values(window.modules.siteOrder.services);
		let favoriteCategoryIds = new Set();
		if (term === 'fav') {
			services.forEach(service => {
				if (service.favorite) {
					favoriteCategoryIds.add(parseInt(service.cid));
				}
			});
		}
		const type100Map = new Map();
		services.forEach(service => {
			if (parseInt(service.type) === 100) {
				const cid = parseInt(service.cid);
				if (!type100Map.has(cid)) {
					type100Map.set(cid, []);
				}
				type100Map.get(cid).push(service);
			}
		});
		let filtered = [];
		if (term === 'auto') {
			filtered = categories.filter(cat => type100Map.has(parseInt(cat.id)));
		} else if (term === 'fav') {
			filtered = categories.filter(cat => favoriteCategoryIds.has(parseInt(cat.id)));
		} else if (term === 'new') {
			filtered = categories.filter(cat => cat.id !== -1);
		} else {
			filtered = categories.filter(cat => cat.name.toLowerCase().includes(term.toLowerCase()));
		}
		if (country) {
			const lowerCountry = country.toLowerCase();
			filtered = filtered.filter(cat => {
				if (cat.name.toLowerCase().includes(lowerCountry))
					return true;
				return services.some(s => s.cid == cat.id && (s.name || '').toLowerCase().includes(lowerCountry));
			});
		}
		if (!filtered.length) {
			if (term === 'fav') {
				return '<option disabled selected>No favorites available</option>';
			}
			if (term === 'auto') {
				return '<option disabled selected>No auto services available</option>';
			}
			filtered = categories.filter(cat => cat.id !== -1);
		}
		return filtered.map(cat => `<option value="${cat.id}">${cat.name}</option>`).join('');
	}

	function getCountryOptions() {
		const services = Object.values(window.modules.siteOrder.services || {});
		const allNames = [
			...categories.map(c => c.name),
			...services.map(s => s.name || s.service || '')
		];
		const countryList = getCountryList();
		const countries = new Set();
		countryList.forEach(country => {
			const lower = country.toLowerCase();
			if (allNames.some(name => name.toLowerCase().includes(lower))) {
				countries.add(country);
			}
		});
		if (!countries.size) {
			return '<option disabled selected>No countries available</option>';
		}
		return Array.from(countries).sort().map(c => `<option value="${c}">${c}</option>`).join('');
	}

	function getCountryList() {
		return COUNTRIES;
	}

	function getIcon(serviceName) {
		for (const keyword in iconMapping) {
			if (serviceName.includes(keyword)) {
				return iconMapping[keyword];
			}
		}
		return iconMapping["default"];
	}
