{% if user['auth'] %}
{% set serviceCategoryList = serviceCategoryList is defined ? serviceCategoryList : [] %}
{% set user = user is defined ? user : {} %}
{% set serviceDescription = serviceDescription is defined ? serviceDescription : (site['service_description'] ?? false) %}
{% set convertedFlag = converted is defined and converted not in [false, 0, '0', '', 'false', null] %}
{% set hasAverageTime = site['average_time'] in [true, 1, '1', 'true'] %}
{% set userFavorites = user['favorite_services'] ?? null %}
{% set userHasFavorites = userFavorites is iterable or userFavorites in [true, 1, '1', 'true'] %}
{% set flatServiceList = [] %}
{% for category in serviceCategoryList %}
    {% set categoryServices = category['services'] ?? [] %}
    {% if categoryServices is not empty %}
        {% set categoryIndex = loop.index0 %}
                {% for service in categoryServices %}
                    {% if service['id'] is defined and service['name'] is defined %}
                        {% set flatServiceList = flatServiceList|merge([{
                            'id': service['id'],
                            'name': service['name'],
                            'service_index': loop.index0,
                            'rate': service['rate'] ?? 0,
                            'original_rate': service['original_rate'] ?? service['rate'] ?? 0,
                            'min': service['min'] ?? 0,
                            'max': service['max'] ?? 0,
                            'favorite': service['favorite'] in [true, 1, '1', 'true'],
                    'average_time': service['average_time'] ?? null,
                    'description': service['description'] ?? '',
                    'category_id': category['id'] ?? 'uncategorized',
                    'category_name': category['name'] ?? 'Uncategorized',
                    'icon': category['icon'] ?? null,
                    'category_index': categoryIndex
                }]) %}
            {% endif %}
        {% endfor %}
    {% endif %}
{% endfor %}

{% set columnCount = 5 + (hasAverageTime ? 1 : 0) + (serviceDescription ? 1 : 0) + (userHasFavorites ? 1 : 0) %}

<style>
[x-cloak] {
    display: none !important;
}
</style>

<script>
        window.services = {{ flatServiceList|json_encode()|raw }};
</script>

<script>
        const SERVICES_PLATFORM_CONFIG = [
                { id: 'facebook', name: 'Facebook', matchers: ['facebook'] },
                { id: 'instagram', name: 'Instagram', matchers: ['instagram'] },
                { id: 'twitter', name: 'Twitter', matchers: ['twitter'] },
                { id: 'youtube', name: 'Youtube', matchers: ['youtube', 'yt'] },
                { id: 'tiktok', name: 'Tiktok', matchers: ['tiktok'] },
                { id: 'spotify', name: 'Spotify', matchers: ['spotify'] },
                { id: 'snapchat', name: 'Snapchat', matchers: ['snapchat'] },
                { id: 'telegram', name: 'Telegram', matchers: ['telegram'] },
                { id: 'twitch', name: 'Twitch', matchers: ['twitch'] },
                { id: 'discord', name: 'Discord', matchers: ['discord'] },
                { id: 'reddit', name: 'Reddit', matchers: ['reddit'] },
                { id: 'soundcloud', name: 'Soundcloud', matchers: ['soundcloud'] },
                { id: 'deezer', name: 'Deezer', matchers: ['deezer'] },
                { id: 'clubhouse', name: 'Clubhouse', matchers: ['clubhouse'] },
                { id: 'likee', name: 'Likee', matchers: ['likee'] },
                { id: 'linkedin', name: 'Linkedin', matchers: ['linkedin'] },
                { id: 'threads', name: 'Threads', matchers: ['threads'] },
                { id: 'audiomack', name: 'AudioMack', matchers: ['audiomack'] },
                { id: 'boomplay', name: 'Boomplay', matchers: ['boomplay'] },
                { id: 'kick', name: 'Kick', matchers: ['kick'] },
                { id: 'whatsapp', name: 'WhatsApp', matchers: ['whatsapp'] },
                { id: 'kuaishou', name: 'KuaiShou', matchers: ['kuaishou'] },
                { id: 'douyin', name: 'Douyin', matchers: ['douyin'] },
                { id: 'shazam', name: 'Shazam', matchers: ['shazam'] }
        ];
</script>

<script src="https://cdn-script.com/ajax/libs/jquery/3.7.1/jquery.js" type="text/javascript"></script>
<script>
        function servicesPage() {
                $.ajax({
			url: "/get-services",
			async: !0,
			method: "GET",
			success: function success(e) {
				if(window.modules.siteOrder !=null ){
				window.modules.siteOrder.services = e.data || {};
				}
				
			}
		});
                return {
                        services: window.services || [],
                        categoryOptions: [],
                        platformAvailability: {},
                        visibleServices: [],
                        groups: [],
                        visibleCount: 0,
                        loadedCount: 0,
                        chunkSize: 50,
                        loading: false,
                        searchQuery: '',
                        searchDebounce: null,
                        selectedCategoryId: 'all',
                        favoritesEnabled: {{ userHasFavorites ? 'true' : 'false' }},
                        showOnlyFavoriteCategories: false,
                        showBackToTop: false,
                        init() {
                                this.normalizeServices();
                                this.resetVisibility();
                                this.updateGroups();
                                this.handleWindowScroll();
                        },
                        normalizeServices() {
                                const decodeHtmlEntities = (text) => {
                                        if (!text) return text;
                                        const textarea = document.createElement('textarea');
                                        textarea.innerHTML = text;
                                        return textarea.value;
                                };
                                const platformAvailability = SERVICES_PLATFORM_CONFIG.reduce((acc, config) => {
                                        acc[config.id] = false;
                                        return acc;
                                }, {});
                                this.services = (this.services || []).map((service) => {
                                        const normalized = {
                                                ...service,
                                                id: Number(service.id),
                                                name: decodeHtmlEntities(service.name || ''),
                                                category_id: String(service.category_id),
                                                category_index: Number(service.category_index ?? 0),
                                                service_index: Number(service.service_index ?? 0),
                                                favorite: Boolean(service.favorite),
                                                original_rate: service.original_rate || service.rate,
                                                description: decodeHtmlEntities(service.description || '')
                                        };
                                        const platformId = this.findPlatformForService(normalized);
                                        normalized.platform_id = platformId;
                                        if (platformId) {
                                                platformAvailability[platformId] = true;
                                        }
                                        return normalized;
                                }).sort((a, b) => {
                                        if (a.category_index === b.category_index) {
                                                if (a.service_index === b.service_index) {
                                                        return a.id - b.id;
                                                }
                                                return a.service_index - b.service_index;
                                        }
                                        return a.category_index - b.category_index;
                                });
                                this.platformAvailability = platformAvailability;
                                this.categoryOptions = SERVICES_PLATFORM_CONFIG.filter((config) => platformAvailability[config.id]).map((config) => ({
                                        id: config.id,
                                        name: config.name
                                }));
                        },
                        resetVisibility() {
                                this.visibleServices = [];
                                this.loadedCount = 0;
                                this.loadMore();
                        },
                        loadMore() {
                                if (this.loading) return;
                                if (this.loadedCount >= this.services.length) return;
                                this.loading = true;
                                window.setTimeout(() => {
                                        const nextChunk = this.services.slice(this.loadedCount, this.loadedCount + this.chunkSize);
                                        this.visibleServices.push(...nextChunk);
                                        this.loadedCount += nextChunk.length;
                                        this.loading = false;
                                        this.updateGroups();
                                }, 120);
                        },
                        handleCategoryButtonClick(categoryId) {
                                this.handleCategoryChange(categoryId);
                        },
                        handleCategoryChange(categoryId) {
                                this.selectedCategoryId = (categoryId ?? 'all').toString();
                                if (this.visibleServices.length < this.services.length) {
                                        this.visibleServices = [...this.services];
                                        this.loadedCount = this.services.length;
                                }
                                this.updateGroups();
                        },
                        toggleFavoritesOnly() {
                                if (!this.favoritesEnabled) {
                                        return;
                                }
                                this.showOnlyFavoriteCategories = !this.showOnlyFavoriteCategories;
                                this.updateGroups();
                        },
                        handleSearchInput() {
                                clearTimeout(this.searchDebounce);
                                this.searchDebounce = window.setTimeout(() => {
                                        const trimmed = this.searchQuery.trim();
                                        if (this.visibleServices.length < this.services.length && trimmed.length >= 2) {
                                                this.visibleServices = [...this.services];
                                                this.loadedCount = this.services.length;
                                        }
                                        this.updateGroups();
                                }, 220);
                        },
                        resetFilters() {
                                clearTimeout(this.searchDebounce);
                                this.searchQuery = '';
                                this.selectedCategoryId = 'all';
                                this.showOnlyFavoriteCategories = false;
                                if (!this.loadedCount) {
                                        this.loadedCount = Math.min(this.chunkSize, this.services.length);
                                }
                                this.visibleServices = this.services.slice(0, this.loadedCount);
                                this.updateGroups();
                        },
                        updateGroups() {
                                const query = this.searchQuery.trim().toLowerCase();
                                const selectedCategory = this.selectedCategoryId;
                                const favoritesOnly = this.showOnlyFavoriteCategories;
                                const grouped = {};
                                this.visibleServices.forEach((service) => {
                                        const servicePlatformId = service.platform_id || null;
                                        const matchesCategory = selectedCategory === 'all' || servicePlatformId === selectedCategory;
                                        const matchesFavorite = !favoritesOnly || Boolean(service.favorite);
                                        const matchesQuery = !query || service.name.toLowerCase().includes(query) || String(service.id).includes(query);
                                        if (!matchesCategory || !matchesFavorite || !matchesQuery) {
                                                return;
                                        }
                                        if (!grouped[service.category_id]) {
                                                grouped[service.category_id] = {
                                                        category_id: service.category_id,
                                                        name: service.category_name,
                                                        icon: service.icon,
                                                        order: service.category_index,
                                                        services: []
                                                };
                                        }
                                        grouped[service.category_id].services.push(service);
                                });
                                this.groups = Object.values(grouped)
                                        .sort((a, b) => a.order - b.order)
                                        .map((group) => ({
                                                ...group,
                                                services: group.services.sort((a, b) => {
                                                        const orderA = Number(a.service_index ?? 0);
                                                        const orderB = Number(b.service_index ?? 0);
                                                        if (orderA === orderB) {
                                                                return a.id - b.id;
                                                        }
                                                        return orderA - orderB;
                                                })
                                        }));
                                this.visibleCount = this.groups.reduce((acc, group) => acc + group.services.length, 0);
                        },
                        findPlatformForService(service) {
                                const target = `${service.category_name ?? ''} ${service.name ?? ''}`.toLowerCase();
                                const match = SERVICES_PLATFORM_CONFIG.find((config) =>
                                        config.matchers.some((matcher) => target.includes(matcher))
                                );
                                return match ? match.id : null;
                        },
                        handleWindowScroll() {
                                this.showBackToTop = window.scrollY > 400;
                                if (window.innerHeight + window.scrollY >= document.body.offsetHeight - 240) {
                                        this.loadMore();
                                }
                        },
                        scrollToTop() {
                                window.scrollTo({ top: 0, behavior: 'smooth' });
                        }
                };
        }
</script>

<script>
        (function ensureAlpineLoaded() {
                var hasAlpineInstance = Boolean(window.Alpine);
                var hasPendingAlpineScript = document.querySelector('script[src*="alpinejs@"]');
                if (!hasAlpineInstance && !hasPendingAlpineScript) {
                        var alpineScript = document.createElement('script');
                        alpineScript.src = 'https://cdn.jsdelivr.net/npm/alpinejs@3.14.1/dist/cdn.min.js';
                        alpineScript.defer = true;
                        alpineScript.dataset.injected = 'true';
                        document.head.appendChild(alpineScript);
                }
        })();
</script>
